{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
		"no-shadow": ["error", { "builtinGlobals": false, "hoist": "functions", "allow": ["cb", "req", "res", "next", "done"] }],
		"no-console": ["off"]
	}
}
